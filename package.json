{
  "name": "tsgrep",
  "version": "3.10.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "jiti": "^2.5.1",
    "peggy": "^5.0.6",
    "prettier": "^3.6.2",
    "ts-jest": "29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "worker-loader": "^3.0.8"
  },
  "scripts": {
    "build-parser": "peggy -o src/parser.js --format es src/grammar.peggy",
    "prebuild": "rm -rf .tsbuildinfo && rm -rf dist",
    "build": "npm run build-parser && webpack --mode=production",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "pretest": "npm run build",
    "test": "jest --maxWorkers=50% --coverage --verbose"
  },
  "keywords": [
    "AST Parsing",
    "Search"
  ],
  "author": "Shantanu Goyal<shantanugoyal23@gmail.com>",
  "license": "ISC",
  "description": "CLI tool that leverages TypeScript's Abstract Syntax Tree (AST) to perform precise and efficient searches",
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "commander": "^14.0.1",
    "fast-glob": "^3.3.3"
  }
}
